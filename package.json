{
    "name": "login",
    "version": "0.0.0",
    "description": "React implementation of Keycloak Login v2 for Keycloakify",
    "repository": {
        "type": "git",
        "url": "git://github.com/keycloakify/login.git"
    },
    "scripts": {
        "build": "tsx scripts/build.ts",
        "_format": "prettier '**/*.{ts,tsx,json,md}'",
        "format": "npm run _format -- --write",
        "format:check": "npm run _format -- --list-different"
    },
    "lint-staged": {
        "*.{ts,tsx,json,md}": [
            "prettier --write"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged -v"
        }
    },
    "author": "u/garronej",
    "license": "MIT",
    "keywords": [],
    "homepage": "https://github.com/keycloakify/login",
    "dependencies": {
        "tsafe": "^1.8.5"
    },
    "peerDependencies": {
        "keycloakify": "^11.8.27"
    },
    "devDependencies": {
        "@types/node": "^22.15.18",
        "typescript": "^5.8.3",
        "husky": "^4.3.8",
        "lint-staged": "^11.1.1",
        "prettier": "^3.3.2",
        "tsx": "^4.19.4",
        "react": "^18.3.1",
        "@types/react": "^19.1.4",
        "keycloakify": "^11.8.27",
        "zod": "^3.24.4"
    },
    "publishConfig": {
        "access": "public"
    }
}
